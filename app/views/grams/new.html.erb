<% if @gram.errors.present? %>
  <div>
      <%= @gram.errors.full_messages %>
  </div>
<% end %>

<%= form_for @gram do |f| %>
<!--
  <div>
    <%= time_ago_in_words(grams.created_at) %>
  </div> -->

  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>


  <div>
    <%= f.label :location_code %>
    <%= f.country_select :location_code, include_blank: true, priority_countries: ["SG","MY"], selected: "SG" %>
  </div>

  <div>
    <%= f.submit %>
  </div>


<% end %>



